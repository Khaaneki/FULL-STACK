SELECT e.nom AS employe_nom, e.salaire AS employe_salaire, p.nom AS patron_nom, p.salaire AS patron_salaire FROM employes AS e JOIN employes AS p ON e.id_patron = p.id WHERE e.salaire > p.salaire;

SELECT e.nom AS nom, e.salaire AS salaire, e.departement AS departement
FROM employe e WHERE e.salaire > (SELECT MAX(salaire) FROM employe WHERE nodep = 31 )ORDER BY e.nodep, e.salaire;

